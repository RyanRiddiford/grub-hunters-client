onent.js","original":{"line":74,"column":67}},{"generated":{"line":61,"column":889},"source":"components/forms/report.component.js","original":{"line":74,"column":68},"name":"getItem"},{"generated":{"line":61,"column":896},"source":"components/forms/report.component.js","original":{"line":74,"column":75}},{"generated":{"line":61,"column":897},"source":"components/forms/report.component.js","original":{"line":74,"column":76}},{"generated":{"line":61,"column":913},"source":"components/forms/report.component.js","original":{"line":74,"column":92}},{"generated":{"line":61,"column":914},"source":"components/forms/report.component.js","original":{"line":74,"column":93}},{"generated":{"line":62,"column":2},"source":"components/forms/report.component.js","original":{"line":82,"column":2}},{"generated":{"line":63,"column":0},"source":"components/forms/report.component.js","original":{"line":83,"column":0}},{"generated":{"line":63,"column":1},"source":"components/forms/report.component.js","original":{"line":83,"column":1}},{"generated":{"line":63,"column":2},"source":"components/forms/report.component.js","original":{"line":83,"column":2}}],"sources":{"components/forms/report.component.js":"/**\r\n * Report form component\r\n * \r\n * Author: Ryan Riddiford\r\n * Student ID: 20862086\r\n */\r\n\r\n\r\n//Import dependencies\r\nimport { LitElement, html, css } from '@polymer/lit-element';\r\nimport {anchorRoute, gotoRoute} from './../../Router';\r\nimport Auth from '../../services/AuthAPI';\r\nimport App from './../../App';\r\n\r\n\r\n//Define the header element as a custom element\r\ncustomElements.define('report-form', class ReportForm extends LitElement {\r\n\r\n\r\n  constructor(){\r\n    console.log(\"constructing report form\");\r\n    super()    \r\n  }\r\n\r\n  //Configure the element's custom properties\r\n  static get properties(){\r\n    console.log(\"setting properties\");\r\n    return {\r\n      title: {\r\n        type: String\r\n      },\r\n      submitBtn: {\r\ntype: HTMLElement\r\n      }\r\n    }\r\n  }\r\n\r\n    firstUpdated(){\r\n    super.firstUpdated();\r\n  }\r\n\r\n\r\n  async createReportSubmitHandler(e){\r\n    //TODO\r\n    e.preventDefault()\r\n    const formData = e.detail.formData\r\n    const submitBtn = document.querySelector('#report-submit-btn')\r\n    submitBtn.setAttribute('loading', '')\r\n    try {\r\n      const response = await ReportAPI.create(formData)      \r\n      //delete updatedUser.password           \r\n      //Auth.currentUser = updatedUser\r\n      //this.render()\r\n      console.log(response);\r\n      Toast.show('report created')\r\n    }catch(err){      \r\n      Toast.show(err, 'error')\r\n    }\r\n    submitBtn.removeAttribute('loading')\r\n  }\r\n\r\n\r\n  /**\r\n   * Renders the register form html\r\n   * @returns Render of register form\r\n   */\r\n  render(){  \r\nreturn html`\r\n<sl-form class=\"page-form\" @sl-submit=${this.createReportSubmitHandler.bind(this)}>\r\n<div class=\"input-group\">\r\n<sl-input type=\"hidden\" name=\"authorId\" value=\"${Auth.currentUser._id}\" ></sl-input>\r\n\r\n<sl-input type=\"hidden\" name=\"targetId\" value=\"${localStorage.getItem('restaurantId')}\" ></sl-input>\r\n<sl-input type=\"hidden\" name=\"restaurantName\" value=\"${localStorage.getItem('restaurantName')}\" ></sl-input>\r\n<sl-input type=\"text\" name=\"topic\" placeholder=\"Topic\" ></sl-input>\r\n<sl-input type=\"text\" name=\"text\" placeholder=\"Text\" ></sl-input>\r\n<sl-input type=\"hidden\" name=\"userType\" value=\"2\" ></sl-input>\r\n\r\n</div>\r\n      <sl-button id=\"report-submit-btn\" type=\"primary\" class=\"submit-btn\" submit>Post Report</sl-button>\r\n    </sl-form>`;\r\n  }\r\n});"},"lineCount":null}},"error":null,"hash":"615f146ecdc79e35fbf6bc3c87627705","cacheData":{"env":{}}}