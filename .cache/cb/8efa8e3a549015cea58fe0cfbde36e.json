 buttons\");\n      this.enableButton(nextBtnSelector);\n    }\n    //Viewing last page -> disable next buttons\n    if (this.currPage + 1 == numPages) {\n      console.log(\"Viewing last page -> disable next buttons\");\n      this.disableButton(nextBtnSelector);\n    }\n  }\n\n  /**\r\n   * Disable a button\r\n   * @param {*} querySelector Stylesheet selector of buttons\r\n   */\n  disableButton(querySelector) {\n    const pageBtns = document.querySelectorAll(querySelector);\n    pageBtns.forEach(el => {\n      el.classList.add(\"hidden\");\n    });\n  }\n\n  /**\r\n   * Enable a button\r\n   * @param {*} querySelector Stylesheet selector of buttons\r\n   */\n  enableButton(querySelector) {\n    const pageBtns = document.querySelectorAll(querySelector);\n    console.log(\"enabling buttons\");\n    console.log(pageBtns);\n    pageBtns.forEach(el => {\n      el.classList.remove(\"hidden\");\n    });\n  }\n}\nvar _default = new PaginationUtils();\nexports.default = _default;"},"sourceMaps":{"js":{"mappings":[{"generated":{"line":7,"column":0},"source":"utils/pagination.utils.js","original":{"line":1,"column":0}},{"generated":{"line":8,"column":0},"source":"utils/pagination.utils.js","original":{"line":2,"column":0}},{"generated":{"line":9,"column":0},"source":"utils/pagination.utils.js","original":{"line":3,"column":0}},{"generated":{"line":10,"column":0},"source":"utils/pagination.utils.js","original":{"line":4,"column":0}},{"generated":{"line":11,"column":0},"source":"utils/pagination.utils.js","original":{"line":5,"column":0}},{"generated":{"line":12,"column":0},"source":"utils/pagination.utils.js","original":{"line":6,"column":0}},{"generated":{"line":14,"column":0},"source":"utils/pagination.utils.js","original":{"line":8,"column":0}},{"generated":{"line":15,"column":0},"source":"utils/pagination.utils.js","original":{"line":9,"column":0}},{"generated":{"line":15,"column":6},"source":"utils/pagination.utils.js","original":{"line":9,"column":6},"name":"Pagination