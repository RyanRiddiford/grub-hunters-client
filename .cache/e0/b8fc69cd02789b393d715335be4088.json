column":51},"generated":{"line":1,"column":4530}},{"source":"components/header.component.js","original":{"line":172,"column":52},"generated":{"line":1,"column":4541}},{"source":"components/header.component.js","original":{"line":173,"column":36},"generated":{"line":1,"column":4557}},{"source":"components/header.component.js","name":"Auth","original":{"line":173,"column":42},"generated":{"line":1,"column":4561}},{"source":"components/header.component.js","original":{"line":173,"column":46},"generated":{"line":1,"column":4563}},{"source":"components/header.component.js","name":"signOut","original":{"line":173,"column":47},"generated":{"line":1,"column":4571}},{"source":"components/header.component.js","original":{"line":182,"column":30},"generated":{"line":1,"column":4581}},{"source":"components/header.component.js","name":"menuClick","original":{"line":182,"column":35},"generated":{"line":1,"column":4586}},{"source":"components/header.component.js","original":{"line":183,"column":37},"generated":{"line":1,"column":4596}},{"source":"components/header.component.js","name":"menuClick","original":{"line":183,"column":42},"generated":{"line":1,"column":4601}},{"source":"components/header.component.js","original":{"line":184,"column":30},"generated":{"line":1,"column":4611}},{"source":"components/header.component.js","name":"Auth","original":{"line":184,"column":36},"generated":{"line":1,"column":4615}},{"source":"components/header.component.js","original":{"line":184,"column":40},"generated":{"line":1,"column":4617}},{"source":"components/header.component.js","name":"signOut","original":{"line":184,"column":41},"generated":{"line":1,"column":4625}}],"sources":{"components/header.component.js":"import { LitElement, html, css } from '@polymer/lit-element'\r\nimport {anchorRoute, gotoRoute} from './../Router'\r\nimport Auth from './../Auth'\r\nimport App from './../App'\r\n\r\ncustomElements.define('header', class AppHeader extends LitElement {\r\n  constructor(){\r\n    super()    \r\n  }\r\n\r\n  static get properties(){\r\n    return {\r\n      title: {\r\n        type: String\r\n      },\r\n      user: {\r\n        type: Object\r\n      }\r\n    }\r\n  }\r\n\r\n  firstUpdated(){\r\n    super.firstUpdated()\r\n    this.navActiveLinks()    \r\n  }\r\n\r\n  navActiveLinks(){\t\r\n    const currentPath = window.location.pathname\r\n    const navLinks = this.shadowRoot.querySelectorAll('.app-top-nav a, .app-side-menu-items a')\r\n    navLinks.forEach(navLink => {\r\n      if(navLink.href.slice(-1) == '#') return\r\n      if(navLink.pathname === currentPath){\t\t\t\r\n        navLink.classList.add('active')\r\n      }\r\n    })\r\n  }\r\n\r\n  hamburgerClick(){  \r\n    const appMenu = this.shadowRoot.querySelector('.app-side-menu')\r\n    appMenu.show()\r\n  }\r\n  \r\n  menuClick(e){\r\n    e.preventDefault()\r\n    const pathname = e.target.closest('a').pathname\r\n    const appSideMenu = this.shadowRoot.querySelector('.app-side-menu')\r\n    // hide appMenu\r\n    appSideMenu.hide()\r\n    appSideMenu.addEventListener('sl-after-hide', () => {\r\n      // goto route after menu is hidden\r\n      gotoRoute(pathname)\r\n    })\r\n  }\r\n\r\n  render(){    \r\n    return html`\r\n    <style>      \r\n      * {\r\n        box-sizing: border-box;\r\n      }\r\n      .app-header {\r\n        background: var(--brand-color);\r\n        position: fixed;\r\n        top: 0;\r\n        right: 0;\r\n        left: 0;\r\n        height: var(--app-header-height);\r\n        color: #fff;\r\n        display: flex;\r\n        z-index: 9;\r\n        box-shadow: 4px 0px 10px rgba(0,0,0,0.2);\r\n        align-items: center;\r\n      }\r\n      \r\n\r\n      .app-header-main {\r\n        flex-grow: 1;\r\n        display: flex;\r\n        align-items: center;\r\n      }\r\n\r\n      .app-header-main::slotted(h1){\r\n        color: #fff;\r\n      }\r\n\r\n      .app-logo a {\r\n        color: #fff;\r\n        text-decoration: none;\r\n        font-weight: bold;\r\n        font-size: 1.2em;\r\n        padding: .6em;\r\n        display: inline-block;        \r\n      }\r\n\r\n      .app-logo img {\r\n        width: 90px;\r\n      }\r\n      \r\n      .hamburger-btn::part(base) {\r\n        color: #fff;\r\n      }\r\n\r\n      .app-top-nav {\r\n        display: flex;\r\n        height: 100%;\r\n        align-items: center;\r\n      }\r\n\r\n      .app-top-nav a {\r\n        display: inline-block;\r\n        padding: .8em;\r\n        text-decoration: none;\r\n        color: #fff;\r\n      }\r\n      \r\n      .app-side-menu-items a {\r\n        display: block;\r\n        padding: .5em;\r\n        text-decoration: none;\r\n        font-size: 1.3em;\r\n        color: #333;\r\n      }\r\n\r\n      .app-side-menu-logo {\r\n        width: 120px;\r\n        margin-bottom: 1em;\r\n        position: absolute;\r\n        top: 2em;\r\n        left: 1.5em;\r\n      }\r\n\r\n      .page-title {\r\n        color: var(--app-header-txt-color);\r\n        margin-right: 0.5em;\r\n        font-size: var(--app-header-title-font-size);\r\n      }\r\n\r\n      /* active nav links */\r\n      .app-top-nav a.active,\r\n      .app-side-menu-items a.active {\r\n        font-weight: bold;\r\n      }\r\n\r\n      /* RESPONSIVE - MOBILE ------------------- */\r\n      @media all and (max-width: 768px){       \r\n        \r\n        .app-top-nav {\r\n          display: none;\r\n        }\r\n      }\r\n\r\n    </style>\r\n\r\n    <header class=\"app-header\">\r\n      <sl-icon-button class=\"hamburger-btn\" name=\"list\" @click=\"${this.hamburgerClick}\" style=\"font-size: 1.5em;\"></sl-icon-button>       \r\n      \r\n      <div class=\"app-header-main\">\r\n        ${this.title ? html`\r\n          <h1 class=\"page-title\">${this.title}</h1>\r\n        `:``}\r\n        <slot></slot>\r\n      </div>\r\n\r\n      <nav class=\"app-top-nav\">\r\n        <a href=\"/\" @click=\"${anchorRoute}\">Home</a>        \r\n        <sl-dropdown>\r\n          <a slot=\"trigger\" href=\"#\" @click=\"${(e) => e.preventDefault()}\">\r\n            <sl-avatar style=\"--size: 24px;\" image=${(this.user && this.user.avatar) ? `${App.apiBase}/images/${this.user.avatar}` : ''}></sl-avatar> ${this.user && this.user.firstName}\r\n          </a>\r\n          <sl-menu>            \r\n            <sl-menu-item @click=\"${() => gotoRoute('/profile')}\">Profile</sl-menu-item>\r\n            <sl-menu-item @click=\"${() => gotoRoute('/editProfile')}\">Edit Profile</sl-menu-item>\r\n            <sl-menu-item @click=\"${() => Auth.signOut()}\">Sign Out</sl-menu-item>\r\n          </sl-menu>\r\n        </sl-dropdown>\r\n      </nav>\r\n    </header>\r\n\r\n    <sl-drawer class=\"app-side-menu\" placement=\"left\">\r\n      <img class=\"app-side-menu-logo\" src=\"/images/logo.svg\">\r\n      <nav class=\"app-side-menu-items\">\r\n        <a href=\"/\" @click=\"${this.menuClick}\">Home</a>\r\n        <a href=\"/profile\" @click=\"${this.menuClick}\">Profile</a>\r\n        <a href=\"#\" @click=\"${() => Auth.signOut()}\">Sign Out</a>\r\n      </nav>  \r\n    </sl-drawer>\r\n    `\r\n  }\r\n  \r\n})\r\n\r\n"},"lineCount":null}},"error":null,"hash":"a88b46ec6c5fcee7b5111e00a2381bd8","cacheData":{"env":{}}}