ponent.js","original":{"line":182,"column":75}},{"generated":{"line":39,"column":591},"source":"components/list/restaurant.component.js","original":{"line":183,"column":48}},{"generated":{"line":39,"column":595},"source":"components/list/restaurant.component.js","original":{"line":183,"column":52}},{"generated":{"line":39,"column":596},"source":"components/list/restaurant.component.js","original":{"line":183,"column":53},"name":"restaurant"},{"generated":{"line":39,"column":606},"source":"components/list/restaurant.component.js","original":{"line":183,"column":63}},{"generated":{"line":39,"column":607},"source":"components/list/restaurant.component.js","original":{"line":183,"column":64},"name":"cuisine"},{"generated":{"line":39,"column":614},"source":"components/list/restaurant.component.js","original":{"line":183,"column":71}},{"generated":{"line":40,"column":2},"source":"components/list/restaurant.component.js","original":{"line":195,"column":1}},{"generated":{"line":41,"column":0},"source":"components/list/restaurant.component.js","original":{"line":198,"column":0}},{"generated":{"line":41,"column":1},"source":"components/list/restaurant.component.js","original":{"line":198,"column":1}},{"generated":{"line":41,"column":2},"source":"components/list/restaurant.component.js","original":{"line":198,"column":2}}],"sources":{"components/list/restaurant.component.js":"/**\r\n * Restaurant Listing Component\r\n * \r\n * Author: Ryan Riddiford\r\n * Student ID: 20862086\r\n */\r\n\r\n\r\n//Import dependencies\r\nimport {\r\n\tLitElement, html\r\n}\r\nfrom '@polymer/lit-element';\r\nimport gotoRoute from '../../Router';\r\nimport AuthAPI from '../../services/AuthAPI';\r\nimport enumUtils from '../../utils/enum.utils';\r\n\r\n\r\n//Define custom element\r\ncustomElements.define('restaurant-listing', class RestaurantListing extends LitElement {\r\n\r\n\r\n\tconstructor() {\r\n\t\tsuper();\r\n\t}\r\n\r\n\t//Configure the element's custom properties\r\n\tstatic get properties() {\r\n\t\treturn {\r\n\t\t\trestaurant: {\r\n\t\t\t\ttype: Object\r\n\t\t\t}\r\n\t\t};\r\n\t}\r\n\r\n\r\n\t/**\r\n\t * Renders a restaurant listing\r\n\t * @returns Render of restaurant listing\r\n\t */\r\n\trender() {\r\n\r\n\t\treturn html `\r\n\r\n<style>\r\n\r\nh3 {\r\n     font-size: var(--h3-font-size);\r\n     font-weight:var(--h3-font-weight);\r\n     font-family:var(--heading-font-family);\r\n     color:var(--light-txt-color);\r\n     margin:10px;\r\n     padding:0px;\r\n     text-align:left;\r\n}\r\n .bold-text {\r\n     font-size:var(--label-font-size);\r\n     font-weight:var(--label-font-weight);\r\n}\r\n .info-header {\r\n    font-size:1.3rem;\r\n    color:var(--light-txt-color);\r\n    background:var(--secondary-brand-color);\r\n    text-align:center;\r\n    border-top-left-radius:10px;\r\n    border-top-right-radius:10px;\r\n }\r\n .restaurant-listing {\r\n     display:flex;\r\n     flex-direction: column;\r\n     width:500px;\r\n     margin:20px;\r\n     box-shadow: var(--main-content-box-shadow);\r\n     border-radius: 20px;\r\n}\r\n\r\n.restaurant-listing .top {\r\n    display:flex;\r\n    flex-direction:row;\r\n    align-items:center;\r\n    justify-content:space-between;\r\n    padding-left:20px;\r\n    padding-right:20px;\r\n\r\n    background:var(--secondary-brand-color);\r\n    border-top-left-radius:20px;\r\n    border-top-right-radius:20px;\r\n}\r\n\r\n\r\n.restaurant-listing .bot {\r\n    display:flex;\r\n    flex-direction:row;\r\nalign-items:center;\r\ngap:50px;\r\npadding:20px;\r\n}\r\n\r\n\r\n .restaurant-listing .bot .left {\r\n     display:flex;\r\n     flex-direction: column;\r\n     align-items:center;\r\n     justify-content:center;\r\n     box-shadow:var(--main-content-box-shadow);\r\n}\r\n.restaurant-listing .bot .mid {\r\n     display:flex;\r\n     flex-direction:column;\r\n     box-shadow:var(--main-content-box-shadow);\r\n     border-radius:10px;\r\n}\r\n.restaurant-listing .bot .mid .info-container {\r\n    padding:20px;\r\n}\r\n sl-button {\r\n     padding:10px;\r\n}\r\n sl-button::part(base) {\r\n     font-size:1rem;\r\n     font-weight:600;\r\n}\r\n sl-avatar {\r\n     --size: 100px;\r\n     margin:10px;\r\n}\r\n @media all and (max-width: 768px){\r\n     .restaurant-listing {\r\n         width:80vw;\r\n    }\r\n     sl-button {\r\n         width:100%;\r\n         height:100px;\r\n    }\r\n     sl-button::part(base) {\r\n         font-size:1rem;\r\n         font-weight:600;\r\n    }\r\n     .mid {\r\n         border:none;\r\n         margin:0;\r\n         padding:0;\r\n    }\r\n     sl-avatar {\r\n         --size: 80px;\r\n         margin: 1em;\r\n    }\r\n}\r\n\r\n  \r\n\r\n</style>\r\n\r\n\r\n\r\n<div class=\"restaurant-listing\">\r\n\r\n<div class=\"top\">\r\n  <h3>${this.restaurant.restaurantName}</h3> \r\n  <sl-button class=\"view-btn\" @click=${(event) => {\r\n   AuthAPI.currentRestaurant = this.restaurant;\r\n   localStorage.setItem(\"currentRestaurant\", JSON.stringify(AuthAPI.currentRestaurant));\r\n gotoRoute('/restaurant');\r\n    }}>View Restaurant</sl-button>\r\n</div>\r\n\r\n\r\n<div class=\"bot\">\r\n\r\n  <div class=\"left\"> \r\n     ${AuthAPI.currentRestaurant && AuthAPI.currentRestaurant.avatar ? html` <sl-avatar shape=\"rounded\"\r\n\t\timage=${\r\n\t\t\t(AuthAPI.currentRestaurant && AuthAPI.currentRestaurant.avatar) ? `${enumUtils.BUCKET_URI}/${AuthAPI.currentUser.avatar}` : ''\r\n\t\t} > </sl-avatar>\r\n\t\t`:html` <sl-avatar shape=\"rounded\"></sl-avatar>\r\n\t\t`}\r\n  </div>\r\n\r\n  <div class=\"mid\">\r\n    <div class=\"bold-text info-header\">Information</div>\r\n  <div class=\"info-container\">\r\n    <div><span class=\"bold-text\">Location </span>${this.restaurant.location}</div>\r\n  <div><span class=\"bold-text\">Cuisine </span>${this.restaurant.cuisine}</div>\r\n  </div>\r\n</div>\r\n\r\n\r\n\r\n</div>\r\n\r\n</div>\r\n\r\n`;\r\n\r\n\t}\r\n\r\n\r\n});"},"lineCount":null}},"error":null,"hash":"f021903eccb931f35f2ded2638139259","cacheData":{"env":{}}}