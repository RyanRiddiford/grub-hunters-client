eview.component.js","original":{"line":66,"column":81},"name":"bind"},{"generated":{"line":60,"column":335},"source":"components/forms/edit/review.component.js","original":{"line":66,"column":85}},{"generated":{"line":60,"column":336},"source":"components/forms/edit/review.component.js","original":{"line":66,"column":86}},{"generated":{"line":60,"column":340},"source":"components/forms/edit/review.component.js","original":{"line":66,"column":90}},{"generated":{"line":60,"column":342},"source":"components/forms/edit/review.component.js","original":{"line":66,"column":92}},{"generated":{"line":60,"column":346},"source":"components/forms/edit/review.component.js","original":{"line":66,"column":96}},{"generated":{"line":60,"column":347},"source":"components/forms/edit/review.component.js","original":{"line":66,"column":97},"name":"review"},{"generated":{"line":60,"column":353},"source":"components/forms/edit/review.component.js","original":{"line":66,"column":103}},{"generated":{"line":60,"column":354},"source":"components/forms/edit/review.component.js","original":{"line":66,"column":104},"name":"_id"},{"generated":{"line":60,"column":357},"source":"components/forms/edit/review.component.js","original":{"line":66,"column":107}},{"generated":{"line":60,"column":358},"source":"components/forms/edit/review.component.js","original":{"line":66,"column":108}},{"generated":{"line":60,"column":360},"source":"components/forms/edit/review.component.js","original":{"line":67,"column":14},"name":"content"},{"generated":{"line":60,"column":367},"source":"components/forms/edit/review.component.js","original":{"line":67,"column":21}},{"generated":{"line":61,"column":2},"source":"components/forms/edit/review.component.js","original":{"line":71,"column":2}},{"generated":{"line":62,"column":0},"source":"components/forms/edit/review.component.js","original":{"line":73,"column":0}},{"generated":{"line":62,"column":1},"source":"components/forms/edit/review.component.js","original":{"line":73,"column":1}},{"generated":{"line":62,"column":2},"source":"components/forms/edit/review.component.js","original":{"line":73,"column":2}}],"sources":{"components/forms/edit/review.component.js":"/**\r\n * Edit profile form component\r\n * \r\n * Author: Ryan Riddiford\r\n * Student ID: 20862086\r\n */\r\n\r\n\r\n//Import dependencies\r\nimport { LitElement, html, css } from '@polymer/lit-element';\r\nimport {anchorRoute, gotoRoute} from '../../../Router';\r\nimport Auth from '../../../services/AuthAPI';\r\nimport App from '../../../App';\r\nimport ReviewAPI from '../../../services/ReviewAPI';\r\n\r\n\r\n//Define custom element\r\ncustomElements.define('edit-review-form', class EditReviewForm extends LitElement {\r\n\r\n\r\n  constructor(){\r\n    super();    \r\n  }\r\n\r\n  //Configure the element's custom properties\r\n  static get properties(){\r\n    return {\r\n      title: {\r\n        type: String\r\n      },\r\n      review: {\r\n        type: Object\r\n      },\r\n    }\r\n  }\r\n\r\n  /**\r\n   * Handler for edit review form\r\n   * @param {*} e \r\n   * @param {*} reviewId \r\n   */\r\n  async updateReviewSubmitHandler(e, reviewId){\r\n    e.preventDefault()\r\n    const formData = e.detail.formData;\r\n    const submitBtn = document.querySelector('.submit-btn');\r\n    submitBtn.setAttribute('loading', '');\r\n    try {\r\n      const updatedReview = await ReviewAPI.updateById(reviewId, formData, false);      \r\n      console.log(updatedReview);\r\n      this.render();\r\n      Toast.show('review updated');\r\n    }catch(err){      \r\n      Toast.show(err, 'error');\r\n    }\r\n    submitBtn.removeAttribute('loading');\r\n  }\r\n\r\n\r\n  /**\r\n   * Renders the edit review form\r\n   * @returns Render of edit review form\r\n   */\r\n  render(){\r\n\r\n    return html`\r\n          <sl-form class=\"page-form\" @sl-submit=${this.updateReviewSubmitHandler.bind(this, this.review._id)}>\r\n            ${content}\r\n            <sl-button type=\"primary\" class=\"submit-btn\" submit>Update Review</sl-button>\r\n          </sl-form>\r\n    `;\r\n  }\r\n\r\n});"},"lineCount":null}},"error":null,"hash":"3f5e0caecf2297a77af8dc829fbbbbed","cacheData":{"env":{}}}