pages/login.js","original":{"line":97,"column":31}}],"sources":{"views/pages/login.js":"\r\n//Import  dependencies\r\nimport App from './../../App';\r\nimport {html, render } from 'lit-html';\r\nimport {anchorRoute, gotoRoute} from './../../Router';\r\nimport AuthAPI from '../../services/AuthAPI';\r\nimport Utils from './../../utils/ui.utils';\r\n\r\n\r\n\r\n/**\r\n * Login page which contains the login and register forms\r\n */\r\nclass LoginView {\r\n\r\n  /**\r\n   * Initialise the page\r\n   */\r\n  init(){\r\n    console.log('LoginView.init');\r\n    document.title = 'Login';\r\n    this.render();\r\n    Utils.pageIntroAnim();\r\n  }\r\n\r\n\r\n  /**\r\n   * Render the page's html\r\n   */\r\n  render(){    \r\n\r\n    const header = html`<app-header title=\"Login\" user=\"${JSON.stringify(AuthAPI.currentUser)}\"></app-header>`;\r\n    \r\n    const loginForm = html`<login-form title=\"Login\"></login-form>`;\r\n    const registerForm = html`<register-form title=\"Register\"></register-form>`;\r\n\r\n    const template = html`    \r\n\r\n\r\n<style>\r\n\r\n  sl-tab-group {\r\n    --indicator-color:gray;\r\n  }\r\n\r\nsl-tab-group::part(base) {\r\n  width:700px;\r\n  border: 4px solid #6F7A82;\r\n  border-radius:20px;\r\n \r\n\r\n}\r\n\r\nsl-tab-group::part(body) {\r\n    /* border-left: 4px solid #6F7A82;\r\n  border-right: 4px solid #6F7A82;\r\n  border-bottom: 4px solid #6F7A82; */\r\n   padding:20px;\r\n}\r\n\r\nsl-tab {\r\n  width:50%;\r\ntext-align:center;\r\n\r\n}\r\n\r\nsl-tab:hover {\r\n  cursor:pointer;\r\n}\r\n\r\n\r\n\r\n\r\n\r\n\r\n</style>\r\n\r\n\r\n    ${header}\r\n\r\n    <sl-tab-group  class=\"page-content\">\r\n    <sl-tab slot=\"nav\" panel=\"login\"><h2>Login</h2></sl-tab>\r\n    <sl-tab slot=\"nav\" panel=\"register\"><h2>Register</h2></sl-tab>\r\n  <sl-tab-panel name=\"login\">          \r\n  ${loginForm}\r\n  </sl-tab-panel>\r\n  <sl-tab-panel name=\"register\">\r\n${registerForm}\r\n  </sl-tab-panel>\r\n    </sl-tab-group> \r\n    <app-footer title=${document.title}></app-footer>\r\n    `\r\n    render(template, App.rootEl)    \r\n  }\r\n}\r\n\r\n export default new LoginView()     \r\n"},"lineCount":null}},"error":null,"hash":"e951d9242590e9c3f74d85aee9b5e35c","cacheData":{"env":{}}}