},{"source":"views/pages/introduction.js","name":"JSON","original":{"line":176,"column":44},"generated":{"line":1,"column":4352}},{"source":"views/pages/introduction.js","name":"JSON","original":{"line":176,"column":44},"generated":{"line":1,"column":4353}},{"source":"views/pages/introduction.js","name":"JSON","original":{"line":176,"column":44},"generated":{"line":1,"column":4407}},{"source":"views/pages/introduction.js","name":"JSON","original":{"line":176,"column":44},"generated":{"line":1,"column":4484}},{"source":"views/pages/introduction.js","name":"JSON","original":{"line":176,"column":44},"generated":{"line":1,"column":4537}},{"source":"views/pages/introduction.js","name":"JSON","original":{"line":176,"column":44},"generated":{"line":1,"column":4563}},{"source":"views/pages/introduction.js","name":"stringify","original":{"line":176,"column":49},"generated":{"line":1,"column":4568}},{"source":"views/pages/introduction.js","name":"AuthAPI","original":{"line":176,"column":59},"generated":{"line":1,"column":4578}},{"source":"views/pages/introduction.js","original":{"line":176,"column":66},"generated":{"line":1,"column":4580}},{"source":"views/pages/introduction.js","name":"currentUser","original":{"line":176,"column":67},"generated":{"line":1,"column":4588}},{"source":"views/pages/introduction.js","name":"content","original":{"line":179,"column":2},"generated":{"line":1,"column":4601}},{"source":"views/pages/introduction.js","name":"document","original":{"line":184,"column":26},"generated":{"line":1,"column":4603}},{"source":"views/pages/introduction.js","name":"title","original":{"line":184,"column":35},"generated":{"line":1,"column":4612}},{"source":"views/pages/introduction.js","original":{"line":187,"column":4},"generated":{"line":1,"column":4620}},{"source":"views/pages/introduction.js","name":"render","original":{"line":187,"column":4},"generated":{"line":1,"column":4622}},{"source":"views/pages/introduction.js","original":{"line":187,"column":10},"generated":{"line":1,"column":4624}},{"source":"views/pages/introduction.js","name":"template","original":{"line":187,"column":11},"generated":{"line":1,"column":4632}},{"source":"views/pages/introduction.js","name":"App","original":{"line":187,"column":21},"generated":{"line":1,"column":4634}},{"source":"views/pages/introduction.js","original":{"line":187,"column":24},"generated":{"line":1,"column":4636}},{"source":"views/pages/introduction.js","name":"rootEl","original":{"line":187,"column":25},"generated":{"line":1,"column":4644}},{"source":"views/pages/introduction.js","original":{"line":192,"column":0},"generated":{"line":1,"column":4653}},{"source":"views/pages/introduction.js","name":"_default","original":{"line":192,"column":0},"generated":{"line":1,"column":4657}},{"source":"views/pages/introduction.js","original":{"line":193,"column":15},"generated":{"line":1,"column":4659}},{"source":"views/pages/introduction.js","name":"IntrodoctionView","original":{"line":193,"column":19},"generated":{"line":1,"column":4663}},{"source":"views/pages/introduction.js","name":"exports","original":{"line":193,"column":37},"generated":{"line":1,"column":4665}},{"source":"views/pages/introduction.js","name":"default","original":{"line":193,"column":37},"generated":{"line":1,"column":4673}},{"source":"views/pages/introduction.js","name":"_default","original":{"line":193,"column":37},"generated":{"line":1,"column":4681}}],"sources":{"views/pages/introduction.js":"/**\r\n * Introduction page when user logs in for first time\r\n * \r\n * Author: Ryan Riddiford\r\n * Student ID: 20862086\r\n */\r\n\r\n\r\n//Import dependencies\r\nimport App from './../../App';\r\nimport {html, render } from 'lit-html';\r\nimport AuthAPI from '../../services/AuthAPI';\r\nimport UserAPI from '../../services/UserAPI';\r\nimport enumUtils from '../../utils/enum.utils';\r\n\r\n//Introduction page\r\nclass IntrodoctionView {\r\n\r\n\r\n  /**\r\n   * Initialise the view\r\n   */\r\n  init(){\r\n    document.title = 'Introduction';    \r\n    this.render();\r\n    this.unsetIntroFlag();\r\n  }\r\n\r\n\r\n\r\n/**\r\n * Sets show intro flag for intro page to false\r\n */\r\n  async unsetIntroFlag() {\r\n            try {\r\n      const updatedUser = await UserAPI.updateById(AuthAPI.currentUser._id, JSON.stringify({\"showIntro\":false}), true);      \r\n      delete updatedUser.password;       \r\n      AuthAPI.currentUser = updatedUser;     \r\n      AuthAPI.currentUser = updatedUser;\r\n      console.log(\"removed intro flag from user\");\r\n    }catch(err){      \r\n      console.log(err);\r\n    }  \r\n  }\r\n\r\n\r\n/**\r\n * Template containing guide for new reviewers\r\n */\r\n  reviewerTemplate() {\r\n\r\n    return html`\r\n    <h1>Reviewer Guide</h1>\r\n    <div class=\"overview-container\">\r\n    <h2>Overview</h2>\r\n    <p>\r\n      Welcome ${AuthAPI.currentUser.username}. \r\n      At Grub Hunters, you can view nearby Australian restaurants and make an informed decision on restaurants you dine at!\r\n      Don't forget to leave helpful reviews on restaurants you visit. \r\n      Your voice can improve and support restaurants, and assist other reviewers in finding good grub!\r\n    </p>      \r\n    </div>\r\n\r\n    <div class=\"features-container\">\r\n    <h2>Current Features</h2>  \r\n    <ul>\r\n      <li>Upload a profile image</li>\r\n      <li>Edit profile details</li>\r\n      <li>Delete your account</li>\r\n      <li>View restaurants and reviews</li>\r\n      <li>Create, edit, and delete restaurant reviews</li>\r\n      <li>Vote on other user reviews</li>\r\n    </ul>\r\n    </div>\r\n\r\n    <div class=\"demerits-container\">\r\n    <h2>About Demerits</h2>  \r\n    <p>\r\n      Breaches in the user guidelines can result in the accumulation of demerit points.\r\n      Having 1 demerit point will result in a one-time warning message.\r\n      Having 2 demerit points will result in a 7-day account suspension.\r\n      Having 3 demerit points will result in an indefinite account ban\r\n    </p>      \r\n    </div>\r\n    `;\r\n\r\n  }\r\n\r\n/**\r\n * Template containing guide for new restaurants\r\n */\r\n  restaurantTemplate() {\r\n    return html`\r\n    \r\n    <h1>Restaurant Guide</h1>\r\n    <div class=\"overview-container\">\r\n    <h2>Overview</h2>\r\n    <p>\r\n      Welcome ${AuthAPI.currentUser.username}. \r\n      At Grub Hunters, you can better engage with consumers by having them view your profile and engage with reviews!\r\n    </p>\r\n  </div>\r\n    <div class=\"features-container\">\r\n    <h2>Current Features</h2>  \r\n    <ul>\r\n      <li>Upload a profile image</li>\r\n      <li>Edit profile details</li>\r\n      <li>Delete your account</li>\r\n      <li>View restaurants and reviews</li>\r\n      <li>Create, edit, and delete restaurant reviews</li>\r\n    </ul> \r\n  </div>\r\n    <div class=\"demerits-container\">\r\n    <h2>About Demerits</h2>  \r\n    <p>\r\n      Breaches in the user guidelines can result in the accumulation of demerit points.\r\n      Having 1 demerit point will result in a one-time warning message.\r\n      Having 2 demerit points will result in a 7-day account suspension.\r\n      Having 3 demerit points will result in an indefinite account ban\r\n    </p>      \r\n    </div>\r\n    `;\r\n  }\r\n\r\n\r\n/**\r\n * Template containing guide for new admins\r\n */\r\n  adminTemplate() {\r\n    return html`\r\n    \r\n    <h1>Administrator Guide</h1>\r\n    <div class=\"overview-container\">\r\n    <h2>Overview</h2>\r\n    <p>\r\n      Welcome ${AuthAPI.currentUser.username}. \r\n      At Grub Hunters, you can view nearby Australian restaurants and make an informed decision on restaurants you dine at!\r\n      As an administrator, your main role is to assess report tickets and determine a course of action on them.\r\n      Restaurant public profiles and restaurant reviews are the two targets a report can be about.\r\n    </p>\r\n  </div>\r\n  <div class=\"features-container\">\r\n    <h2>Current Features</h2>  \r\n    <ul>\r\n      <li>Upload a profile image</li>\r\n      <li>Edit profile details</li>\r\n      <li>View restaurants and reviews</li>\r\n      <li>Assess report tickets</li>\r\n    </ul> \r\n  </div>\r\n    `;\r\n  }\r\n\r\n\r\n/**\r\n * Render the view\r\n */\r\n  render(){\r\n\r\n    let content;\r\n\r\n    //Build the reviewer template\r\n    if (AuthAPI.currentUser.accessLevel == enumUtils.accessLevels.reviewer)\r\n    content = this.reviewerTemplate();\r\n    //Build the restaurant template\r\n    else if (AuthAPI.currentUser.accessLevel == enumUtils.accessLevels.restaurant)\r\n    content = this.restaurantTemplate();\r\n    //Build the admin template\r\n    else if (AuthAPI.currentUser.accessLevel == enumUtils.accessLevels.administrator)\r\n    content = this.adminTemplate();\r\n\r\n\r\n    const template = html`   \r\n\r\n\r\n    <app-header title=\"Introduction\" user=${JSON.stringify(AuthAPI.currentUser)}></app-header> \r\n<div class=\"page-content\">\r\n  <div class=\"intro-guide\">\r\n${content}\r\n  </div>\r\n\r\n</div>\r\n  \r\n      <app-footer title=${document.title}></app-footer>\r\n    `;\r\n\r\n    render(template, App.rootEl);\r\n  }\r\n}\r\n\r\n\r\n//Export the view\r\nexport default new IntrodoctionView();"},"lineCount":null}},"error":null,"hash":"fd01d90afe61d0a45f9f240c7ec7c3e1","cacheData":{"env":{}}}